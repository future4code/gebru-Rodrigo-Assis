import {Request, Response} from "express"
import  connection  from "../connection"






export const addUser = async (req: Request, res: Response) => {
   

    try {
        await connection("user")
         
        res.send("Ok");
    } catch(e) {
        console.error({e});
        return res.status(500).send("Algo deu errado.");
    }
};


